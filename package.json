{
  "name": "live-collab",
  "displayName": "live-collab",
  "description": "Live Collaborator of Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:live-collab.helloWorld",
    "onCommand:live-collab.startCollaboration"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "live-collab.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "live-collab.startCollaboration",
        "title": "Start Collaboration"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@types/firebase": "^2.4.32",
    "firebase": "^11.0.2",
    "uuid": "^11.0.3"
  }
}
